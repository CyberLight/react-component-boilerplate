{
  "name": "react-component-boilerplate",
  "version": "0.0.0",
  "description": "React component boilerplate",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/leandrowd/react-component-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/leandrowd/react-component-boilerplate/issues"
  },
  "homepage": "http://leandrowd.github.io/react-component-boilerplate/",
  "scripts": {
    "postinstall": "npm run build",
    "start": "watch 'npm run lint' ./src",
    "lint": "eslint src/",
    "build": "babel src/ --out-dir ./lib",
    "test": "mocha",
    "tdd": "mocha --watch",
    "coverage": "isparta cover _mocha --include-all-sources --report lcovonly",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "clean": "rm build/app.js"
  },
  "author": {
    "name": "Leandro Augusto Lemos",
    "url": "leandrowd@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-class-properties": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.0",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "watch": "^0.16.0"
  },
  "tags": [
    "React",
    "React component",
    "React component boilerplate",
    "Boilerplate",
    "Webpack"
  ],
  "keywords": [
    "React component boilerplate",
    "Boilerplate",
    "Webpack"
  ]
}
